<h1>Sign In</h1>

<a class="Sign-up-link" href="<%= new_user_url %>">Sign Up</a>

<br><br>


<form class="sign-in" action="<%= session_url %>" method="POST">
    <input type="hidden" 
        name="authenticity_token" 
            value="<%= form_authenticity_token %>">

    <label>Email
        <p>
            <input type="email" name="user[email]" value="">
        </p>
    </label>

    <label>Password
        <p>
            <input type="password" name="user[password]" value="">
        </p>
    </label>

    <input type="submit" value="Sign In">

</form>